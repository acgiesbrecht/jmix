<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view"
      xmlns:c="http://jmix.io/schema/flowui/jpql-condition"
      title="msg://newEntityListView.title"
      focusComponent="newEntitiesTable">
    <facets>
        <dataLoadCoordinator auto="true"/>
    </facets>
    <actions>
        <action id="selectAction" type="lookup_select"/>
        <action id="discardAction" type="lookup_discard"/>
    </actions>
    <layout expand="entityTreeFragment" height="500px" spacing="true">
        <formLayout id="treePanel" height="500px">
        </formLayout>
        <fragment id="entityTreeFragment" screen="report_EntityTree.fragment" width="100%"/>
        <hbox id="lookupActions" spacing="true">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</view>
